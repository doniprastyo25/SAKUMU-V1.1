<div class="container-fluid p-0">    
    <div class="row">
        <div class="col">
            <h5><%= title %></h5>
            <h6 class="fw-normal"><%= dess %></h6>
        </div>
        <div class="col-sm text-end me-3">    
            <form action="#" method="post">
                <div class="input-group">
                    <input type="text" class="form-control form-control-sm rounded-start" placeholder="Pencarian" aria-label="Pencarian" id="search" name="search">
                    <button class="btn btn-primary btn-sm rounded-end pe-2" type="submit"><i class="bi bi-search me-2"></i>Cari</button>
                </div>
            </form>
            <div class="mt-3">
                <a href="#" class="btn btn-secondary badge rounded-pill fw-normal px-3 ms-2"><i class="bi bi-list-ul me-2"></i>Tampilkan semua</a>
                <a href="#" class="btn btn-secondary badge rounded-pill fw-normal px-3 ms-2"><i class="bi bi-printer-fill me-2"></i>cetak laporan</a>
            </div>
        </div>
    </div>
    <hr>
    <div class="row">
        <div class="col">
          <div class="btn-group dropend">
            <button type="button" class="btn btn-sm btn-secondary dropdown-toggle btn-warning fw-bold" data-bs-toggle="dropdown" aria-expanded="false">
              Keterangan
            </button>
          </div>
        </div>
        <div class="col-sm-4 text-end me-3">
            <form method="get" action="#">
                <div class="row">
                    <div class="col-sm-3">
                        Filter :
                    </div>
                    <div class="col-sm">
                        <div class="input-group">
                            <input type="date" class="form-control form-control-sm" id="date" name="date" value="#" required>
                            <button type="submit" class="btn btn-primary btn-sm">Terapkan</button>
                        </div>
                    </div>
                </div>    
            </form>
        </div>
    </div>
    <table class="table table-sm table-bordered table-striped mt-3">
        <thead>
            <tr>
              <th class="text-center" scope="col" style="width: 5%;">No</th>
              <th class="text-center" scope="col" style="width: 10%;">Tanggal</th>
              <th class="text-center" scope="col" style="width: 50%;">Uraian</th>
              <th class="text-center" scope="col" style="width: 20%;">Total</th>
              <th class="text-center" scope="col" style="width: 15%;">Aksi</th>
            </tr>
        </thead>
        <tbody> 
                <tr>
                    <td colspan="4" class="text-center" ><span class="dot bg-primary me-2"></span>Pilih Semua</td>
                    <td class="text-center">
                        <input type="checkbox" id="select-one" class="All form-check-input"> All 
                    </td>
                </tr>           
                <tr>
                    <th class="text-center" scope="row">1.2.1</th>
                    <td>02/03/2022</td>
                    <td>tes pembayaran</td>
                    <td><span class="dot bg-primary me-2"></span>Rp. 100.000,00</td>
                    <td class="text-center">
                        <input type="checkbox" class="All select-one form-check-input"> All 
                    </td>
                </tr>           
                <tr>
                    <th class="text-center" scope="row">1.2.2</th>
                    <td>02/03/2022</td>
                    <td>tes pembayaran 2</td>
                    <td><span class="dot bg-primary me-2"></span>Rp. 100.000,00</td>
                    <td class="text-center">
                        <input type="checkbox" class="select-two form-check-input"> All 
                    </td>
                </tr>           
                <tr>
                    <th class="text-center" scope="row">1.2.3</th>
                    <td>02/03/2022</td>
                    <td>tes pembayaran 3</td>
                    <td><span class="dot bg-primary me-2"></span>Rp. 100.000,00</td>
                    <td class="text-center">
                        <input type="checkbox" class="select-one form-check-input"> All 
                    </td>
                </tr>           
        </tbody>    
    </table>


</div>
<script src="/ressources/js/cekprint.js"></script>
  