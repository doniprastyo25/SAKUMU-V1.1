<div class="modal fade" id="listkwitansi" tabindex="-1" aria-labelledby="ModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <div class="modal-header bg-info">
          <h5 class="m-0 p-0 text-white">List Kwitansi</h5>
        </div>
        <div class="modal-body text-start">
            <table class="table table-sm table-bordered table-striped mt-3">
                <thead>
                    <tr>
                      <th class="text-center" scope="col" style="width: 5%;">No</th>
                      <th class="text-center" scope="col" style="width:40%;">Kwitansi</th>
                      <th class="text-center" scope="col" style="width: 20%;">Cetak</th>
                    </tr>
                </thead>
                <tbody> 
                    <% for(let i = 0; i<file.length; i++) {%> 
                        <tr>
                            <th class="text-center" scope="row"><%= 1+i %> </th>
                            <td><%= file[i] %> </td>
                            <td class="text-center">
                               <a href="/penerimaan/<%= sub %>/printkwitansi?file=<%= file[i] %> " class="btn badge btn-primary fw-normal"><i class="bi bi-printer-fill me-2"></i> Cetak </a> 
                            </td>
                        </tr>
                    <%}%>             
                </tbody>    
            </table>
        </div>
        <div class="modal-footer">
        </div>        
      </div>
    </div>
</div>