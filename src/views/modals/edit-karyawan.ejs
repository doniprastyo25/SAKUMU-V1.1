<div class="modal fade" id="editKaryawan<%= data.id %>" tabindex="-1" aria-labelledby="ModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
          <div class="modal-header bg-info">
            <h5 class="m-0 p-0 text-white">Detail Karyawan</h5>
          </div>
          <div class="modal-body text-start">
              <form method="post" action="/gj/addkaryawan">
                  <div class="container-fluid p-0">
                      <div class="card bg-white p-3 mb-3">
                          <div class="row">
                              <div class="col-sm">
                                  <label for="inpID" class="col-sm-2 col-form-label">id</label>
                                  <input type="text" class="form-control form-control-sm" name="inidkaryawan" id="inidkaryawan" autocomplete="off" value="<%= data.id %>" disabled>
                              </div>
                              <div class="col-sm">
                                  <label for="inpNama" class="col-sm-2 col-form-label">nama</label>
                                  <input type="text" class="form-control form-control-sm" name="innamakaryawan" id="innamakaryawan" autocomplete="off" value="<%= data.nama %>">
                              </div>
                          </div>
                          <div class="row">
                              <div class="col-sm">
                                  <label for="inpJabatan" class="col-sm-2 col-form-label">Jabatan</label>
                                  <select class="form-select" aria-label="Default select example" name="injabatan" id="injabatan">
                                    <option selected value="<%= data.kj %>"><%= data.jabatan %></option>
                                    <% for( let i = 0; i < jabatan.length; i++ ) { %>
                                        <option value="<%= jabatan[i].kj %> "><%= jabatan[i].namajabatan %> </option>
                                    <% } %>
                                  </select>
                                </div>
                          </div>
                            <div class="row mt-2">
                              <div class="col-sm">
 
                              </div>
                              <div class="col-sm">
                                <div class="row">
                                    <div class="col-sm text-end">
                                      <a href="#" class="btn badge btn-secondary"><i class="bi bi-trash"></i> Batal</a> 
                                    </div>
                                    <div class="col-sm text-end">
                                      <a href="#" class="btn badge btn-info"><i class="bi bi-trash"></i> Kirim</a> 
                                    </div>
                                    <div class="col-sm text-end">
                                      <a href="#" class="btn badge btn-danger"><i class="bi bi-trash"></i> Hapus</a> 
                                    </div>
                                </div>
                            </div>
                        </div>
                      </div>
                  </div>
              </form>
          </div>
  
          <div class="modal-footer">
              
          </div>        
        </div>
    </div>
</div>